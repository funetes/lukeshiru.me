  "scripts": {
    "build:all:dev": "run-s clean build:dev:*",
    "build:all:prod": "run-s clean build:prod:*",
    "build:dev:back": "tsc",
    "build:dev:front": "NODE_ENV=development webpack -d",
    "build:prod:back": "tsc",
    "build:prod:front": "NODE_ENV=production webpack -p",
    "clean": "rimraf ./build",
    "postinstall": "run-s build:all:prod",
    "start": "node build/back",
    "watch:all:dev": "run-p watch:dev:* start",
    "watch:all:prod": "run-p watch:prod:* start",
    "watch:dev:back": "tsc --watch",
    "watch:dev:front": "NODE_ENV=development webpack -d --watch",
    "watch:prod:back": "tsc --watch",
    "watch:prod:front": "NODE_ENV=production webpack -p --watch"
  },
