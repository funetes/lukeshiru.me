{
  "name": "lukeshiru.me",
  "version": "1.3.2",
  "description": "https://lukeshiru.me",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf src/*.js && rimraf static/build",
    "dev": "tsc --watch & isDev=true nodemon ./src/index.js",
    "build": "tsc",
    "start": "node ./src/index.js",
    "bower-install": "cd static && export HOME=$HOME/app-root/runtime/repo; bower i",
    "polymer-build": "cd static && export HOME=$HOME/app-root/runtime/repo; polymer build",
    "postinstall": "npm run build && npm run bower-install && npm run polymer-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukeshiru/lukeshiru.me.git"
  },
  "keywords": [
    "luke",
    "shiru",
    "lucas",
    "ciruzzi",
    "website",
    "personal",
    "developer",
    "deejay",
    "dj",
    "gamer"
  ],
  "author": "Lucas Ciruzzi <hello@lukeshiru.me>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/lukeshiru/lukeshiru.me/issues"
  },
  "homepage": "https://github.com/lukeshiru/lukeshiru.me#readme",
  "dependencies": {
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.36",
    "@types/express-graphql": "0.0.33",
    "@types/graphql": "^0.10.2",
    "@types/gulp": "^4.0.4",
    "@types/helmet": "0.0.36",
    "@types/node": "^8.0.20",
    "@types/request": "^2.0.0",
    "bower": "^1.8.0",
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "express-graphql": "^0.6.7",
    "graphql": "^0.10.5",
    "helmet": "^3.8.1",
    "json-to-graphql": "^0.1.1",
    "polymer-cli": "^1.4.1",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.6.0"
  }
}
