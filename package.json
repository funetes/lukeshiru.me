  "scripts": {
    "build-all-dev": "npm run -s build-back && npm run -s build-front-dev",
    "build-all-prod": "npm run -s build-back && npm run -s build-front-prod",
    "build-back": "tsc",
    "build-front-dev": "NODE_ENV=development webpack -d",
    "build-front-prod": "NODE_ENV=production webpack -p",
    "clean": "rimraf ./build",
    "postinstall": "npm run -s build-all-prod",
    "start": "node build/back",
    "watch-front-dev": "NODE_ENV=development webpack -d --watch",
    "watch-front-prod": "NODE_ENV=production webpack -p --watch",
    "watch-back": "tsc --watch"
  },
